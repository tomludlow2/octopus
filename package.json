{
  "name": "octopus-rpi-api",
  "version": "1.0.0",
  "description": "Octopus API energy data ingestion and EV charge-event pricing",
  "main": "index.js",
  "scripts": {
    "test": "npm run check:syntax",
    "check:syntax": "node scripts/check-syntax.js",
    "fetch:auto": "node lib/fetchLatestAuto.js",
    "fetch:backfill": "node lib/invokeDataProcessor.js",
    "charge:identify:auto": "node lib/AUTO_CHARGE_EVENT.js",
    "charge:identify:backfill": "node lib/invokeAudiProcessor.js",
    "charge:price:auto": "node lib/autoPriceChargeEvents.js",
    "charge:price:next": "node lib/invokePriceChargeEvent.js",
    "db:gaps": "node pg/view_missing_intervals.js",
    "electric:reprice": "node pg/reprice_historical_electric.js",
    "usage:reprice": "node pg/reprice_historical_usage.js",
    "gaps:import": "node pg/import_missing_intervals.js",
    "db:inspect": "node pg/inspect_schema_permissions.js",
    "fetch:monthly:interactive": "node pg/monthly_backfill_interactive.js",
    "test:basic-notification": "node tests/notification/test_basic_notification.js",
    "test:url-notification": "node tests/notification/test_url_notification.js",
    "usage:last-7-days": "node pg/notify_last_7_days_usage.js",
    "usage:last-month": "node pg/notify_last_month_usage.js",
    "test:error_notification": "node tests/notification/test_error_notification.js",
    "interrogate": "node pg/interrogate_usage.js",
    "server:enable_codex": "node scripts/toggle_codex_auth.js enable",
    "server:disable_codex": "node scripts/toggle_codex_auth.js disable",
    "audit:octopus": "node server/workers/audit-octopus-vs-postgres.js",
    "ha:poll:status": "node tests/poll_for_status.js",
    "ha:poll:power": "node tests/poll_for_power.js",
    "ha:poll:octopus-accumulative": "node tests/poll_for_octopus_accumulative_consumption.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.7",
    "bootstrap": "^5.3.8",
    "chart.js": "^4.4.7",
    "cli-table3": "^0.6.5",
    "csv-parser": "^3.0.0",
    "date-fns": "^4.1.0",
    "express": "^4.21.0",
    "pg": "^8.13.0",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5"
  }
}
